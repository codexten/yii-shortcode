{
  "name": "codexten/yii-shortcode",
  "description": "extented yii shortcode components for codexten packages",
  "keywords": [
    "yii2",
    "codexten"
  ],
  "type": "library",
  "require": {
    "codexten/yii-core": "~2.0.0"
  },
  "autoload": {
    "psr-4": {
      "codexten\\yii\\shortcode\\": "src"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "2.0.x-dev"
    },
    "config-plugin": {
      "shortcode": "config/shortcode.php"
    }
  }
}
